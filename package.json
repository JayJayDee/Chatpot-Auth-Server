    "@types/nodemailer": "^6.1.0",
    "@types/redis": "^2.8.10",
    "@types/request-promise-native": "^1.0.15",
    "@types/swagger-jsdoc": "0.0.2",
    "@types/swagger-ui-express": "^3.0.0",
    "jest": "^23.6.0",
    "nodemon": "^1.18.7",
    "rimraf": "^2.6.2",
    "ts-jest": "^23.10.5",
    "ts-node": "^7.0.1",
    "tslib": "^1.9.3",
    "tslint": "^5.12.0",
    "typescript": "^3.2.1"
  },
